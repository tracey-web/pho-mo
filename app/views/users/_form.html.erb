<%= form_for @user do |f| %>
  <%= f.label :username %>
  <%= f.text_field :username %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :email %>
  <%= f.text_field :email %>
  <%= f.label :password %>
  <%= f.text_field :password %>
  <%= f.label :dob, 'Date of birth' %>
  <%= f.text_field :dob, :placeholder => 'YYYY-MM-DD' %>
  <%= f.label :avatar%>
  <%= f.text_field :avatar %>
  <%= f.label :bio %>
  <%= f.text_field :bio %>
  <%= f.submit %>
<% end %>